mixin slogan(data)
    +el({
        class: 'slogan',
        mix: data.mix,
        mod: data.mod,
        js: data.js,
    })&attributes(attributes)
        +el({
            class: 'slogan__text',
            text: 'Кровельные работы с&nbsp;гарантией 10&nbsp;лет в&nbsp;Симферополе и&nbsp;по Крыму',
        })
        +webp({
            mix: 'slogan__image',
            image: 'slogan',
            imageExp: 'png',
            alt: 'Строитель'
        })
