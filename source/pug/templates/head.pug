mixin head(data)
    -
        var title = pageTitle
        var projectName = "template-gulp"
        var author = "Lorem Lipsum"
        var description = "Test for Sheer"
        var keywords = ""

    head
        meta(charset="utf-8")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        meta(content="IE=edge" http-equiv="X-UA-Compatible")
        //- Title
        title= title

        //- Meta tags
        meta(name="description" content=description)
        meta(name="keywords" content=keywords)

        meta(name="format-detection" content="telephone=no")
        meta(name="format-detection" content="address=no")

        //- fonts https://fonts.google.com
        link(href="https://fonts.googleapis.com/css?family=Exo+2:400,700&amp;subset=cyrillic" rel="stylesheet")

        //- Styles
        link(rel="stylesheet" href="./assets/libs/styles/lib.css")
        link(rel="stylesheet" href="./css/styles.min.css")
